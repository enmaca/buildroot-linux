<settings>
	<lookandfeel>
		<enablerssfeeds>false</enablerssfeeds>
		<font>Default</font>
		<rssedit></rssedit>
		<skin>skin.confluence</skin>
		<skincolors>SKINDEFAULT</skincolors>
		<skinsettings></skinsettings>
		<skintheme>SKINDEFAULT</skintheme>
